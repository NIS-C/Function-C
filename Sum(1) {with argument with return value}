
#include <stdio.h>
int sum(int,int);
int main() 
    {
        int a,b,c;
        printf("Enter a \n");
        scanf("%d",&a);
        printf("Enter b \n");
        scanf("%d",&b);
        
        c=sum(a,b);
        printf("Sum is %d \n",c);
    }
    int sum(int x,int y)
    {
        return x+y;
    }
    
